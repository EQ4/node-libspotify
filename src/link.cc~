#include "common.h"

using namespace v8;
using namespace nsp;

void nsp::init_artist(Handle<Object> target) {
    NODE_SET_METHOD(target, "artist_is_loaded", Artist_Is_Loaded);
    NODE_SET_METHOD(target, "artist_name", Artist_Name);
}
